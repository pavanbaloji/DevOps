<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>ExceptionArchive</TargetDatabaseName>
    <DeployScriptFileName>Avista.ESB.SQL.ExceptionArchive.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=${BizTalkDbServer};Integrated Security=True;Pooling=False</TargetConnectionString>
    <BlockWhenDriftDetected>True</BlockWhenDriftDetected>
    <RegisterDataTierApplication>True</RegisterDataTierApplication>
    <ProfileVersionNumber>1</ProfileVersionNumber>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="BizTalkServerHostAccount">
      <Value>${BizTalkServerHostAccount}</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BizTalkServerHostGroup">
      <Value>${BizTalkServerHostGroup}</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BizTalkServerIsolatedHostAccount">
      <Value>${BizTalkServerIsolatedHostAccount}</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="BizTalkServerIsolatedHostGroup">
      <Value>${BizTalkServerIsolatedHostGroup}</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="DeploymentSsoUserGroup">
      <Value>${DeploymentSsoUserGroup}</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="Environment">
      <Value>${Environment}</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>